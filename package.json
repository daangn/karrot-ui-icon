    "@svgr/plugin-svgo": "^6.0.0",
    "@types/react": "^17.0.37",
    "dotenv": "^10.0.0",
    "got": "^11.8.3",
    "lodash": "^4.17.21",
    "svg2vectordrawable": "^2.8.1",
